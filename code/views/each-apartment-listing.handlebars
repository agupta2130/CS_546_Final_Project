 <section class="my-5" id="nav_about_us" >
        <div>
            <h4 class="display-6 text-center ">Details</h4>
            <hr class="w-25 mx-auto">
            <div class="container">
                <div class="row my-5">
                    <div class="col col-xl-6 col-md-6 col-12">

                        <figure>
                            <img style="max-height: 280px; width: 100%;" class="img-fluid rounded-3 mx-auto d-block" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRW44P24uZ4-GyJXYFfAiJcijO-HCgY3l9WQA&usqp=CAU" alt="" />
