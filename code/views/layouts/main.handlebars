<body>
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <a class="navbar-brand" href="/">Apartment Finder</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">

      <div class="navbar-nav">
        {{! <a class="nav-item nav-link active" href="/">Search apartment
          <span class="sr-only">(current)</span></a>
        <a class="nav-item nav-link active" href="/apartment">Apartments
          <span class="sr-only">(current)</span></a> }}

        {{#if isNotLogin}}
        <a class="nav-item nav-link active" href="/login">Login
          <span class="sr-only">(current)</span></a>
        {{!-- <a class="nav-item nav-link hidden" href="/signup">Sign Up</a> --}}
      {{!--   <a href="/signup">Sign Up</a> --}}

        {{else}}
        {{#if signup}}
        <a class="nav-item nav-link active" href="/login">Login</a>
        <a class="nav-item nav-link active" href="/login">Log Out</a>
        {{else}}
        <a class="nav-item nav-link active" href="/apartment">Apartments
          <span class="sr-only">(current)</span></a>
        <a class="nav-item nav-link active" href="/newApartment">Add New
          Property</a>

        <a class="nav-item nav-link active" href="/login">Log Out</a>

        {{/if}}
        {{/if}}

        {{#if allApartmentListing}}
        <a class="nav-item nav-link" href="/apartment/sortbyrent">Sort by
          rent</a> 
        {{! <a class="nav-item nav-link disabled" href="#">Disabled</a> }}

        {{! <ul class="nav navbar-nav navbar-right">
          <li><a href="/login" class="">login</a>
          </li>
          <li>
            <div class="nav navbar-nav navbar-right"><a class="btn btn-primary btn-small navbar-btn"
                href="/register">Sign up!</a>
            </div>
          </li>
        </ul> }}

        {{! <div class="nav navbar-nav navbar-right"><a class="btn btn-primary btn-small navbar-btn"
            href="/register">Sign up!</a>
        </div> }}

        {{/if}}
      </div>
    </div>
    {{#if username}}
    <a class="userprofile" href="/profile/{{email}}">
       {{username}}
    </a>
    {{/if}}
  </nav>

  <main class="container">
    <h1 class="heading">Discover Your New Home. Helping renters find their perfect fit</h1>
    <div class="row">
      <div class="col">
        {{{body}}}
      </div>
    </div>
  </main>

  <script src="https://code.jquery.com/jquery-2.2.4.min.js"
    integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous"></script>
